<% var name = 'My first ejs'; %>
<h1><%= name %> </h1> 
<p style="color:red;"><%= description %> </p>
<hr/>
<table border="1" width="300">
    <% for(var a = 2; a < 10; a++) { %> 
    <tr>
        <% for(var i = 1; i<10; i++)
        { %>
            <td>
                2 * <%= i %> = <%= 2*i %>
            </td>
            <td>
                3 * <%= i %> = <%= 3*i %>
            </td>
            <td>
                2 * <%= i %> = <%= 2*i %>
            </td>
       <% } %>
     
    </tr>
<% } %>
</table>